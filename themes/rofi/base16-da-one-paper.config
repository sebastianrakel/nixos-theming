! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #c8c8c8, #c8c8c8, #faf0dc

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #c8c8c8, #181818, #c8c8c8, #c8c8c8, #000000
rofi.color-active:  #c8c8c8, #5890f8, #c8c8c8, #c8c8c8, #5890f8
rofi.color-urgent:  #c8c8c8, #de5d6e, #c8c8c8, #c8c8c8, #de5d6e

! Set the desired separator style
rofi.separator-style: solid
